    "name": "SHARKKY SMS",
    "version": "1.3.3.7",
    "description": "ส่ง sms",
    "manifest_version": 2,
    "browser_action":{},
    "background": {
        "scripts": ["index.js"]
    },
    "web_accessible_resources":["index.html"],
    "permissions": [
        "http://*/",
        "https://*/",
        "webRequest",
        "webRequestBlocking"
